{
    "presets": ["@babel/preset-env", "@babel/preset-typescript"],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver",
            {
                "root": "src",
                "extensions": [".js", ".jsx", ".es", ".es6", ".mjs", ".ts"]
            }
        ]
    ],
    "include": ["./src"],
    "exclude": ["**/*.test.*"]
}